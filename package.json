{
  "name": "express-scrape-example",
  "version": "1.0.0",
  "description": "IGDB API scrape application, generated using generator-express-no-stress",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "babel server --out-dir dist --delete-dir-on-start --source-maps inline --copy-files",
    "dev": "nodemon server --exec babel-node --config .nodemonrc.json | pino-pretty",
    "dev:debug": "nodemon server --exec babel-node --config .nodemonrc.json --inspect | pino-pretty",
    "test": "mocha --compilers js:@babel/register --exit",
    "test:debug": "mocha --compilers js:@babel/register --inspect-brk --exit",
    "lint": "eslint -c .eslintrc.json {server,test}/**",
    "lint:fix": "eslint --fix -c .eslintrc.json {server,test}/**",
    "styl:watch": "build/build-development"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "download": "^7.1.0",
    "empty-folder": "^2.0.3",
    "express": "^4.16.4",
    "firebase": "^5.5.7",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.10",
    "nanoid": "^2.0.0",
    "normalize.css": "^8.0.1",
    "pino": "^5.8.0",
    "pug": "^2.0.3",
    "stylus": "^0.54.5",
    "swagger-express-middleware": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^8.2.2",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "mocha": "^5.0.5",
    "nodemon": "^1.18.4",
    "pino-pretty": "^2.2.2",
    "supertest": "^3.0.0"
  },
  "author": "Kyle Kopicki <kyle.kopicki@gmail.com> (https://github.com/kopickik)"
}
